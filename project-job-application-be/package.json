{
    "name": "nodejs-expressjs-mongodb-base-project",
    "private": true,
    "version": "1.0.0",
    "author": "Dev_Tuan",
    "engines": {
        "node": ">=18.x"
    },
    "scripts": {
        "lint": "eslint src --ext js --report-unused-disable-directives --max-warnings 0",
        "clean": "rm -rf build && mkdir build",
        "build-babel": "babel ./src -d ./build/src",
        "build": "npm run clean && npm run build-babel",
        "production": "npm run build && cross-env BUILD_MODE=production node ./build/src/server.js",
        "dev": "cross-env BUILD_MODE=dev nodemon --exec babel-node ./src/server.js --ignore *.json",
        "test": "jest"
    },
    "nodemonConfig": {
        "ignore": [
            "*.json",
            "public/javascripts/*.js"
        ]
    },
    "dependencies": {
        "@aws-sdk/client-s3": "^3.835.0",
        "@aws-sdk/lib-storage": "^3.835.0",
        "@aws-sdk/s3-request-presigner": "^3.835.0",
        "@babel/cli": "^7.22.10",
        "@babel/core": "^7.22.10",
        "@babel/plugin-transform-runtime": "^7.22.10",
        "@babel/preset-env": "^7.22.10",
        "@babel/runtime": "^7.22.10",
        "@elastic/elasticsearch": "^9.0.2",
        "@getbrevo/brevo": "^1.0.1",
        "async-exit-hook": "^2.0.1",
        "aws-sdk": "^2.1692.0",
        "axios": "^1.10.0",
        "bcryptjs": "^3.0.2",
        "cloudinary": "^2.6.0",
        "cookie-parser": "^1.4.7",
        "cors": "^2.8.5",
        "cross-env": "^7.0.3",
        "dayjs": "^1.11.13",
        "dotenv": "^16.5.0",
        "express": "^4.18.2",
        "http-status-codes": "^2.3.0",
        "joi": "^17.13.3",
        "jsonwebtoken": "^9.0.2",
        "mongodb": "^6.14.2",
        "ms": "^2.1.3",
        "multer": "^1.4.5-lts.2",
        "multer-s3": "^3.0.1",
        "node-cron": "^3.0.3",
        "nodemon": "^3.1.0",
        "resend": "^4.5.1",
        "rimraf": "^6.0.1",
        "streamifier": "^0.1.1",
        "supertest": "^7.1.1",
        "uuid": "^11.1.0"
    },
    "devDependencies": {
        "@babel/eslint-parser": "^7.22.10",
        "@babel/node": "^7.22.10",
        "babel-plugin-module-resolver": "^5.0.0",
        "eslint": "^8.47.0",
        "jest": "^30.0.0",
        "mongodb-memory-server": "^10.1.4"
    }
}
